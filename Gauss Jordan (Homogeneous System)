import numpy as np

r = int(input("Rows: "))
c = int(input("Cols: "))

print("Enter augmented matrix:")
a = []
for i in range(r):
    a.append(list(map(float, input().split())))

a = np.array(a, float)

for i in range(r):
    a[i] = a[i] / a[i][i]        # divide pivot row
    
    for j in range(r):
        if i != j:
            a[j] = a[j] - a[j][i] * a[i]    # make column zero

print("\nSolution:", a[:, -1])
